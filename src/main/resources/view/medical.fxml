<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.stage.Stage?>

<fx:root maxHeight="500.0" maxWidth="300.0" resizable="false" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/help_icon.png" />
  </icons>
  <scene>
    <Scene stylesheets="@Fonts.css">

      <HBox alignment="CENTER">
        <children>
               <StackPane prefHeight="150.0" prefWidth="200.0">
                  <children>
                <Label fx:id="helpMessage" text="Label" textAlignment="CENTER" wrapText="true" />
                  </children>
               </StackPane>
        </children>
        <opaqueInsets>
          <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
        </opaqueInsets>
        <padding>
          <Insets bottom="10.0" left="5.0" right="10.0" top="5.0" />
        </padding>
      </HBox>
    </Scene>
  </scene>
</fx:root>
